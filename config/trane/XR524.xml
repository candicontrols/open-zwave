		<Value type="decimal" genre="user" instance="1" index="2" label="CoolSetPoint" units="F" read_only="false" write_only="false" min="0" max="0" value="80" />
	</CommandClass>

  <CommandClass id="68" name="COMMAND_CLASS_THERMOSTAT_FAN_MODE" version="1" request_flags="4" create_vars="true">
